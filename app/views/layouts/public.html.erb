<html lang="en">
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
		<title>Simple CMS <%= "| #{@page_title}" if @page_title %></title>
		<meta name="author" content= "Prem Regmi">
		
		<%= stylesheet_link_tag('public')%>
		<%= stylesheet_link_tag('stylee')%>
		</head>
		<body>
			<div id ="header">
				<h1>Simple CMs
					
					<%= link_to("Login", {:controller => 'access', :action => 'menu'}, :class =>'back-link')%>
					<%= link_to("Home", {:controller => 'public', :action => 'index'}, :class =>'back-link')%></h1>
				
		    
				</div>
				
				<div id="main">
					
					<% if !flash[:notice].blank? %>
					<div class="notice">
						<%= flash[:notice] %>
						</div>
						
						<% end %>
					<div id="navigation">
						<%= render(:partial => 'layouts/navigation', :locals => {:subjects => @subjects})%>
						</div>
						<div id ="navigati">
							<%= render(:partial => 'layouts/ok', :locals => {:sections => @sections})%>
							</div>
							
						<div id="content">
							
							<%= yield %>
							
							</div>
							<div class="clear"></div>
							</div>
							<div id="footer">
								
								
